                      <div class="row mb-3">
                        <label for="instance" class="col-sm-2 col-form-label">ith instance</label>
                        <div class="col-sm-10">
                          <input class="form-control" id = "instance" type="text" name="instance" value="<%= input.instance ? input.instance : '' %>" >
                        </div>
                      </div>
 
      
                 
      
                      <div class="row mb-3">
                        <label class="col-sm-2 col-form-label">action1</label>
                        <div class="col-sm-10">
                          
                          <select name="action1" id="action1"  class="form-select" aria-label="Default select example">
              
                            <% for(let i=0;i<actionArray.length; i++){ %>
              
                
                              <option value="<%= actionArray[i] %>" <%= (input.action1 && input.action1 == actionArray[i]) ? 'selected' : '' %> >  <%= actionArray[i] %></option>
                          <% } %>
                              </select>
                        </div>
                      </div>
      

